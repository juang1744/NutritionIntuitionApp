{
    "expo": {
        "name": "NutritionIntuitionApp",
        "slug": "NutritionIntuitionApp",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./media/react-default/icon.png",
        "splash": {
            "image": "./media/react-default/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./media/react-default/adaptive-icon.png",
                "backgroundColor": "#FFFFFF"
            }
        },
        "web": {
            "favicon": "./media/react-default/favicon.png"
        }
    }
}
